<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/twonew"
    tools:context=".LevelTwo">

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="370dp"
        android:layout_height="409dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/lvlgrnd"
            android:layout_width="370dp"
            android:layout_height="138dp"
            android:layout_gravity="bottom"
            android:visibility="invisible"
            app:srcCompat="?attr/colorBackgroundFloating" />

        <ImageView
            android:id="@+id/obs1"
            android:layout_width="57dp"
            android:layout_height="47dp"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="180dp"
            app:srcCompat="@drawable/obs1" />

        <ImageView
            android:id="@+id/obs2"
            android:layout_width="57dp"
            android:layout_height="47dp"
            android:layout_marginLeft="215dp"
            android:layout_marginTop="95dp"
            app:srcCompat="@drawable/obs1" />

        <ImageView
            android:id="@+id/wiz2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginTop="240dp"
            android:layout_marginLeft="5dp"
            app:srcCompat="@drawable/idle" />

        <ImageView
            android:id="@+id/goal"
            android:layout_width="35dp"
            android:layout_height="75dp"
            android:layout_marginTop="36dp"
            android:layout_marginLeft="317dp"
            android:visibility="invisible"
            app:srcCompat="@android:color/background_dark" />
    </FrameLayout>

    <ImageView
        android:id="@+id/right2"
        android:layout_width="93dp"
        android:layout_height="54dp"
        android:layout_marginTop="341dp"
        app:layout_constraintEnd_toStartOf="@+id/frameLayout2"
        app:layout_constraintStart_toEndOf="@+id/left2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/right" />

    <ImageView
        android:id="@+id/left2"
        android:layout_width="52dp"
        android:layout_height="27dp"
        android:layout_marginTop="356dp"
        app:layout_constraintEnd_toStartOf="@+id/right2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/left" />

    <ImageView
        android:id="@+id/up2"
        android:layout_width="55dp"
        android:layout_height="60dp"
        android:layout_marginTop="340dp"
        app:layout_constraintEnd_toStartOf="@+id/down2"
        app:layout_constraintStart_toEndOf="@+id/frameLayout2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/up" />

    <ImageView
        android:id="@+id/down2"
        android:layout_width="55dp"
        android:layout_height="60dp"
        android:layout_marginTop="340dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/up2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/down" />
</androidx.constraintlayout.widget.ConstraintLayout>